{
  "name": "reverse-proxy-rate-limiter",
  "version": "0.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/prezi/reverse-proxy-rate-limiter.git"
  },
  "bugs": {
    "url": "https://github.com/prezi/reverse-proxy-rate-limiter/issues"
