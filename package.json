    "lodash": "^3.3.1",
    "log4js": "0.6.22",
    "nomnom": "^1.8.1",
    "proxy-addr": "1.0.6",
    "request": "2.51.0"
  },
  "devDependencies": {
    "expect.js": "*",
    "mocha": "*"
  },
  "engines": {
    "node": "0.10.35"
  },
  "scripts": {
    "start": "node start-rate-limiter.js",
    "test": "mocha --recursive"
  },
  "license": "Apache License, Version 2.0",
  "jshintConfig": {
    "undef": true,
    "unused": true,
    "node": true,
    "globals": {
